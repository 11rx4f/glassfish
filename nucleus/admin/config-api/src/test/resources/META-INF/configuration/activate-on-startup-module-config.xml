<service-config>
    <config-bundle
            location="domain/configs/config[server-config]"
            replace-if-exist="true">
        <customization-tokens/>
        <configuration-element
                config-bean-class-name="com.sun.enterprise.config.serverbeans.SystemProperty">
            <![CDATA[
<system-property name="startup.overriding.property" value="new-custom-value"></system-property>
]]>
        </configuration-element>
    </config-bundle>

</service-config>


