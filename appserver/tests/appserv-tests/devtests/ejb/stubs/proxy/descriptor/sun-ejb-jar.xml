<sun-ejb-jar>
  <enterprise-beans>
    <ejb>
      <ejb-name>PerformanceApp</ejb-name>
      <jndi-name>ejb/ejb_stubs_proxy_ProxyApp</jndi-name>
    </ejb>
    <ejb>
      <ejb-name>BmpBean</ejb-name>
      <jndi-name>ejb/ejb_stubs_proxy_Bmp</jndi-name>
    </ejb>
    <ejb>
      <ejb-name>SlessBean</ejb-name>
      <jndi-name>ejb/ejb_stubs_proxy_Sless</jndi-name>
      <pass-by-reference>true</pass-by-reference>
    </ejb>
    <ejb>
      <ejb-name>SfulBean</ejb-name>
      <jndi-name>ejb/ejb_stubs_proxy_Sful</jndi-name>
      <pass-by-reference>false</pass-by-reference>
    </ejb>
  </enterprise-beans>
</sun-ejb-jar>
